<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="quartz" type="System.Configuration.NameValueFileSectionHandler" />
  </configSections>

  <quartz>
    <add key="quartz.plugin.recentHistory.type" value="Quartz.Plugins.Quartzmon.RecentHistory.ExecutionHistoryPlugin, Quartz.Plugins.Quartzmon.RecentHistory" />
    <add key="quartz.plugin.recentHistory.storeType" value="Quartz.Plugins.Quartzmon.RecentHistory.Impl.InProcExecutionHistoryStore, Quartz.Plugins.Quartzmon.RecentHistory" />

    <add key="quartz.plugin.quartzmon.type" value="Quartzmon.SelfHost.QuartzmonPlugin, Quartzmon.SelfHost" />
    <add key="quartz.plugin.quartzmon.url" value="http://*:333" />
    <add key="quartz.plugin.quartzmon.useLocalTime" value="true" />
    <add key="quartz.plugin.quartzmon.defaultDateFormat" value="dd/MM/yyyy" />
  </quartz>
</configuration>
